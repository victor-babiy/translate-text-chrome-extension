{
  "name": "Translate selected text",
  "version": "0.0.1",
  "description": "Translate selected text",
  "manifest_version": 2,
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["app.js"]
    }
  ]
}